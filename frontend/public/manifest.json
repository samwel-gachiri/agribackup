{
  "name": "AgriBackup",
  "short_name": "AgriBackup",
  "start_url": "/home",
  "display": "standalone",
  "display_override": [
    "fullscreen",
    "window-controls-overlay",
    "browser"
  ],
  "orientation": "portrait",
  "background_color": "#f5fdf4",
  "theme_color": "#2e7d32",
  "description": "A portal for farmers to manage their activities and resources.",
  "icons": [
    {
      "src": "img/icons2/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "img/icons2/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "img/icons2/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "img/icons2/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/img/screenshots/screenshot1.jpg",
      "sizes": "540x720",
      "type": "image/png",
      "label": "Dashboard"
    }
  ],
  "shortcuts": [
    {
      "name": "Community",
      "short_name": "Community",
      "description": "near farmers and buyers",
      "url": "/community",
      "icons": [
        {
          "src": "/img/icons/android-chrome-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+farmer",
      "url": "/handle-protocol?url=%s"
    }
  ]
}